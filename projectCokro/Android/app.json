        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "cokro.MyApp1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "315e3d23-9731-4ca4-9109-032c3e046e3c"
      }
    }
  }
}
